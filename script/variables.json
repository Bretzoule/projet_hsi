      "declaration": "bool",
      "domain": "val == 0 || val == 1"
    },

    {
      "name": "crc8_t",
      "type": "atom*",
      "declaration": "uint8_t",
      "domain": "val >= 0"
    },
    {
      "name": "tramenb_t",
