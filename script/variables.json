      "domain": "val >= 0 && val <= 40"
    },
    {
      "name": "rpm_t",
      "type": "atom*",
      "declaration": "uint32_t",
      "domain": "value <= 10000"
    },
    {
      "name": "batteryissue_t",
      "type": "enum",
      "declaration": ["noBatteryIssue", "empty", "failure"],
      "domain": "val < 3 && val >= 0"
    },
    {
      "name": "isOnIndicator",
      "type": "atom*",
      "declaration": "bool",
      "domain": "val == 0 || val == 1"
    },
    {
      "name": "isAcquited_t",
      "type": "atom*",
      "declaration": "bool",
      "domain": "val == 0 || val == 1"
    }
